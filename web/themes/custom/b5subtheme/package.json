  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "bootswatch": "^5.3.6",
    "browser-sync": "^3.0.4",
    "browser-sync-webpack-plugin": "^2.4.0",
    "imagemin": "^9.0.1",
    "imagemin-cli": "^8.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^6.0.49",
    "laravel-mix-purgecss": "^6.0.0",
    "sass": "^1.89.2",
    "sass-loader": "^12.6.0"
