{
  "name": "b5subtheme",
  "version": "1.0.0",
  "description": "[Bootstrap 5](https://www.drupal.org/project/bootstrap5) subtheme.",
  "main": "index.js",
  "scripts": {
    "dev": "npx mix",
    "watch": "npx mix watch",
    "watch-poll": "npx mix watch -- --watch-options-poll=1000",
    "hot": "npx mix watch --hot",
    "prod": "npx mix --production",
    "optimize-images": "node optimize-images.js",
    "build": "npm run prod && npm run optimize-images",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
